{
  "env": {
    "lib": {
      "ignore": [ "src/bin/" ],
      "presets": [
        ["env", { "targets": {"browsers": ["last 2 versions"]} }]
      ]
    },
    "cli": {
      "ignore": ["src/*.js"],
    }
  },
  "presets": [
    ["env", {
      "targets": {"node": "6.0"},
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    "transform-object-rest-spread"
  ]
}
